     <attr-item tags="imp"/>

     <attr-item tags="inf"/>
     <attr-item tags="ger"/>
     <attr-item tags="supn"/>
     <attr-item tags="pp"/>
     <attr-item tags="ppres"/>
  </def-attr>

  <def-attr n="gen">
     <attr-item tags="m"/>
     <attr-item tags="f"/>
     <attr-item tags="nt"/>
