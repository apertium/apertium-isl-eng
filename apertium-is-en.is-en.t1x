  <def-macro n="nom_article1" npar="1">
    <let>
      <var n="article"/>
      <lit v=""/>
    </let>
    <choose>
         <when><!-- noun with definitive article -->
           <test>
             <and>
               <equal>
                 <clip pos="1" side="sl" part="a_art"/>
                 <lit-tag v="def"/>
               </equal>
               <not>
                 <or>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.org"/>
                   </equal>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.top"/>
                   </equal>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.ant"/>
                   </equal>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.al"/>
                   </equal>
                 </or>
               </not>
             </and>
           </test>
           <modify-case>
             <clip pos="1" side="tl" part="lem"/>
             <lit v="aa"/>
           </modify-case>
           <let>
             <var n="article"/>
             <concat>
                 <lit v="^"/>
                   <lit v="the"/>
                 <!--<get-case-from pos="1">
                 </get-case-from> -->
                 <lit-tag v="det.def"/>
                 <clip pos="1" side="tl" part="nbr" link-to="3"/>
                 <lit v="$ "/>
             </concat>
           </let> 
         </when>

         <when><!-- proper noun with definitive article -->
           <test>
             <and>
               <or>
                 <in caseless="yes">
                   <clip pos="1" side="tl" part="lem"/>
                   <list n="definitenames"/>
                 </in>
                 <equal>
                   <clip pos="1" side="sl" part="a_art"/>
                   <lit-tag v="def"/>
                 </equal>
               </or>
               <or>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.org"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.top"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.ant"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.al"/>
                 </equal>
               </or>
             </and>
           </test>
           <choose>
             <when>
               <test>
                 <equal> 
                   <var n="EOS"/> 
                   <lit v="true"/> 
                 </equal> 
               </test>
               <let>
                 <var n="article"/>
                 <concat>
                     <lit v="^"/>
                     <get-case-from pos="1">
                       <lit v="the"/>
                     </get-case-from>
                     <lit-tag v="det.def"/>
                     <clip pos="1" side="tl" part="nbr" link-to="3"/>
                     <lit v="$ "/>
                 </concat>
               </let> 
             </when>
             <otherwise>
               <let>
                 <var n="article"/>
                 <concat>
                     <lit v="^"/>
                       <lit v="the"/>
                     <!--<get-case-from pos="1">
                     </get-case-from>-->
                     <lit-tag v="det.def"/>
                     <clip pos="1" side="tl" part="nbr" link-to="3"/>
                     <lit v="$ "/>
                 </concat>
               </let> 
             </otherwise>
           </choose>
         </when>
   </choose>
  </def-macro>

