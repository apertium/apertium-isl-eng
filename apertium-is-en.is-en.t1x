  <rule comment="REGLA: Past Participle"><!-- rule for past participles-->
     <pattern>
       <pattern-item n="pp"/>
     </pattern>
     <action>
       <call-macro n="firstWord">
         <with-param pos="1"/>
       </call-macro>
           <out>
             <chunk name="pp" case="caseFirstWord">
               <tags>
                 <tag><lit-tag v="SA"/></tag>
                 <tag><clip pos="1" side="tl" part="syntax"/></tag>
               </tags>
              <lu>
                 <clip pos="1" side="tl" part="lemh"/>
