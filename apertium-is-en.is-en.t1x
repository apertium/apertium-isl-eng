  <def-var n="genere"/>
  <def-var n="article"/>
  <def-var n="genitiu"/>

  <def-var n="cur_adj_grau1"/>

  <def-var n="cur_verb_lem"/>
  <def-var n="cur_verb_fin"/>
  <def-var n="cur_subj_prn"/>
  <def-var n="cur_subj_gen" v="&lt;m&gt;"/>
  <def-var n="cur_sent_adv"/>
  <def-var n="cur_adv_lema"/>
  <def-var n="cur_prep_lema"/>
</section-def-vars>

<section-def-macros>
  <def-macro n="reset_variables" npar="1">
    <let><var n="number"/>       <lit v=""/></let>
    <let><var n="genere"/>       <lit v=""/></let>
    <let><var n="article"/>      <lit v=""/></let>
    <let><var n="genitiu"/>      <lit v=""/></let>
    <let><var n="cur_verb_lem"/> <lit v=""/></let>
    <let><var n="cur_verb_fin"/> <lit v=""/></let>
    <let><var n="cur_subj_prn"/> <lit v=""/></let>
    <let><var n="cur_sent_adv"/> <lit v=""/></let>
    <let><var n="cur_adv_lema"/> <lit v=""/></let>
    <let><var n="cur_prep_lema"/><lit v=""/></let>
  </def-macro>

  <def-macro n="set_cur_adj_grau1" npar="1">
      <let>
        <var n="cur_adj_grau1"/>
        <lit v=""/>
      </let>
      <choose>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="sl" part="grau"/>
                <lit-tag v="comp"/>
              </equal>
            </and>
          </test>
          <append n="cur_adj_grau1">
            <lit v="more"/>
            <lit-tag v="preadv"/>
            <lit v="$ ^"/>
          </append>
          <let>
            <clip pos="1" side="tl" part="grau"/>
            <lit v=""/>
          </let>
        </when>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="sl" part="grau"/>
                <lit-tag v="sup"/>
              </equal>
            </and>
          </test>
          <append n="cur_adj_grau1">
            <lit v="most"/>
            <lit-tag v="preadv"/>
            <lit v="$ ^"/>
          </append>
          <let>
            <clip pos="1" side="tl" part="grau"/>
            <lit v=""/>
          </let>
        </when>
        <otherwise>
          <append n="cur_adj_grau1">
            <lit v=""/>
          </append>
        </otherwise>
      </choose>
  </def-macro>

  <def-macro n="conj_verb1" npar="1">
      <let>
        <var n="cur_verb_lem"/>
        <clip pos="1" side="sl" part="lem"/>
      </let>

      <choose>
        <when>
          <test>
            <or>
              <equal>
                <clip pos="1" side="tl" part="a_verb"/>
                <lit-tag v="vblex.midv"/>
              </equal>
              <equal>
                <clip pos="1" side="tl" part="a_verb"/>
                <lit-tag v="vblex.actv"/>
              </equal>
            </or>
          </test>
          <let>
            <clip pos="1" side="tl" part="a_verb"/>
            <lit-tag v="vblex"/>
          </let>
        </when>
      </choose>


      <choose>
        <when>
          <test>
            <equal caseless="yes">
              <clip pos="1" side="sl" part="lem"/>
              <lit v="vera"/>
            </equal>
          </test>
          <choose> <!-- Verb is 'vera' (to be) -->
            <when>
              <test>
                <or> <!-- Person is 'p2.sg', 'p2.pl', 'p1.pl' or 'p3.pl' -->
                  <equal>
                    <clip pos="1" side="sl" part="pers"/>
                    <lit-tag v="p2"/>
                  </equal>
                  <and>
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="pers"/>
                        <lit-tag v="p1"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="pers"/>
                        <lit-tag v="p3"/>
                      </equal>
                    </or> 
                    <equal>
                      <clip pos="1" side="sl" part="nbr"/>
                      <lit-tag v="pl"/>
                    </equal>
                  </and> 
                </or>
              </test>           
              <choose>
                <when>
                  <test> <!-- If the tense is 'pri' or 'prs' -->
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="pri"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="prs"/>
                      </equal>
                    </or>
                  </test> 
                  <let> <!-- Set the tense to 'pres' -->
                    <clip pos="1" side="tl" part="temps"/>
                    <lit-tag v="pres"/>
                  </let>
                </when>

                <when>
                  <test> <!-- If the tense is 'pss' -->
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="past"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="pss"/>
                      </equal>
                    </or>
                  </test> 
                  <let> <!-- Set the tense to 'pres' -->
                    <clip pos="1" side="tl" part="temps"/>
                    <lit-tag v="past"/>
                  </let>
                </when>
             </choose>
             <let>
               <var n="cur_verb_fin"/>
               <concat>
                 <clip pos="1" side="tl" part="lemh"/>
                 <clip pos="1" side="tl" part="a_verb"/>
                 <clip pos="1" side="tl" part="temps"/>
                 <clip pos="1" side="tl" part="lemq"/>
               </concat>
             </let>         
            </when>

            <when>
              <test> <!-- Verb 'vera' in p3.sg or p1.sg -->
                <and>
                  <or>
                    <equal>
                      <clip pos="1" side="sl" part="pers"/>
                      <lit-tag v="p3"/>
                    </equal>
                    <equal>
                      <clip pos="1" side="sl" part="pers"/>
                      <lit-tag v="p1"/>
                    </equal>
                  </or>
                  <equal>
                    <clip pos="1" side="sl" part="nbr"/>
                    <lit-tag v="sg"/>
                  </equal>
                </and>
              </test>           
              <choose>
                <when>
                  <test> <!-- If the tense is 'pri' or 'prs' -->
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="pri"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="prs"/>
                      </equal>
                    </or>
                  </test> 
                  <let> <!-- Set the tense to 'pres' -->
                    <clip pos="1" side="tl" part="temps"/>
                    <lit-tag v="pri"/>
                  </let>
                  <let>
                    <var n="cur_verb_fin"/>
                    <concat>
                      <clip pos="1" side="tl" part="lemh"/>
                      <clip pos="1" side="tl" part="a_verb"/>
                      <clip pos="1" side="tl" part="temps"/>
                      <clip pos="1" side="tl" part="pers"/>
                      <clip pos="1" side="tl" part="nbr"/>
                      <clip pos="1" side="tl" part="lemq"/>
                    </concat>
                  </let>     
                </when>
                <when>
                  <test> <!-- If the tense is 'pss' -->
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="past"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="pss"/>
                      </equal>
                    </or>
                  </test> 
                  <let> <!-- Set the tense to 'pres' -->
                    <clip pos="1" side="tl" part="temps"/>
                    <lit-tag v="past"/>
                  </let>
                  <let>
                    <var n="cur_verb_fin"/>
                    <concat>
                      <clip pos="1" side="tl" part="lemh"/>
                      <clip pos="1" side="tl" part="a_verb"/>
                      <clip pos="1" side="tl" part="temps"/>
                      <clip pos="1" side="tl" part="pers"/>
                      <clip pos="1" side="tl" part="nbr"/>
                      <clip pos="1" side="tl" part="lemq"/>
                    </concat>
                  </let>         
                </when>
              </choose>
            </when>

          </choose>
        </when>
      </choose>
      <choose>
        <when> 
          <test>
            <not>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="vera"/>
              </equal>
            </not>
          </test>
          <choose>
            <when> 
              <test>
                <and>
                  <or>
                    <equal>
                      <clip pos="1" side="sl" part="temps"/>
                      <lit-tag v="pri"/>
                    </equal>
                    <equal>
                      <clip pos="1" side="sl" part="temps"/>
                      <lit-tag v="prs"/>
                    </equal>
                  </or>
                  <equal>
                    <clip pos="1" side="sl" part="pers"/>
                    <lit-tag v="p3"/>
                  </equal>
                  <equal>
                    <clip pos="1" side="sl" part="nbr"/>
                    <lit-tag v="sg"/>
                  </equal>
                </and>
              </test>
              <let>
                <clip pos="1" side="tl" part="temps"/>
                <lit-tag v="pri"/>
              </let>
              <let>
                <var n="cur_verb_fin"/>
                <concat>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <clip pos="1" side="tl" part="pers"/>
                  <clip pos="1" side="tl" part="nbr"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </concat>
              </let>            
            </when>
            <when> 
              <test>
                <or>
                  <equal>
                    <clip pos="1" side="sl" part="temps"/>
                    <lit-tag v="pri"/>
                  </equal>
                  <equal>
                    <clip pos="1" side="sl" part="temps"/>
                    <lit-tag v="prs"/>
                  </equal>
                </or>
              </test>
              <let>
                <clip pos="1" side="tl" part="temps"/>
                <lit-tag v="pres"/>
              </let>
              <let>
                <var n="cur_verb_fin"/>
                <concat>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </concat>
              </let>            
            </when>
            <when> 
              <test>
                    <or>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="past"/>
                      </equal>
                      <equal>
                        <clip pos="1" side="sl" part="temps"/>
                        <lit-tag v="pss"/>
                      </equal>
                    </or>
              </test>
              <let>
                <clip pos="1" side="tl" part="temps"/>
                <lit-tag v="past"/>
              </let>
              <let>
                <var n="cur_verb_fin"/>
                <concat>
                  <clip pos="1" side="tl" part="lemh"/>
                  <clip pos="1" side="tl" part="a_verb"/>
                  <clip pos="1" side="tl" part="temps"/>
                  <clip pos="1" side="tl" part="lemq"/>
                </concat>
              </let>      
            </when>
          </choose>
        </when>
      </choose>

      <choose>
        <when>
          <test>
            <or>
              <equal>
                <clip pos="1" side="sl" part="temps"/>
                <lit-tag v="imp"/>
              </equal>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="skulu"/>
              </equal>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="mega"/>
              </equal>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="geta"/>
              </equal>
              <equal caseless="yes">
                <clip pos="1" side="sl" part="lem"/>
                <lit v="munu"/>
              </equal>
            </or>
          </test>
          <let>
            <clip pos="1" side="tl" part="temps"/>
            <lit-tag v="inf"/>
          </let>
          <let>
            <var n="cur_verb_fin"/>
            <concat>
              <clip pos="1" side="tl" part="lemh"/>
              <clip pos="1" side="tl" part="a_verb"/>
              <clip pos="1" side="tl" part="temps"/>
              <clip pos="1" side="tl" part="lemq"/>
            </concat>
          </let>      
        </when>
      </choose>
  </def-macro>

  <def-macro n="nom_article1" npar="1">
    <let>
      <var n="article"/>
      <lit v=""/>
    </let>
    <choose>
         <when><!-- noun with definitive article -->
           <test>
             <and>
               <equal>
                 <clip pos="1" side="sl" part="a_art"/>
                 <lit-tag v="def"/>
               </equal>
               <not>
                 <or>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.org"/>
                   </equal>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.top"/>
                   </equal>
                   <equal>
                     <clip pos="1" side="sl" part="a_nom"/>
                     <lit-tag v="np.ant"/>
                   </equal>
                 </or>
               </not>
             </and>
           </test>
           <modify-case>
             <clip pos="1" side="tl" part="lem"/>
             <lit v="aa"/>
           </modify-case>
           <let>
             <var n="article"/>
             <concat>
                 <lit v="^"/>
                 <get-case-from pos="1">
                   <lit v="the"/>
                 </get-case-from>
                 <lit-tag v="det.def"/>
                 <clip pos="1" side="tl" part="nbr" link-to="2"/>
                 <lit v="$ "/>
             </concat>
           </let> 
         </when>

         <when><!-- proper noun with definitive article -->
           <test>
             <and>
               <equal>
                 <clip pos="1" side="sl" part="a_art"/>
                 <lit-tag v="def"/>
               </equal>
               <or>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.org"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.top"/>
                 </equal>
                 <equal>
                   <clip pos="1" side="sl" part="a_nom"/>
                   <lit-tag v="np.ant"/>
                 </equal>
               </or>
             </and>
           </test>
           <let>
