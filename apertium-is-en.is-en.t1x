    <let><var n="cur_prn_lema"/>       <lit v=""/></let>
    <let><var n="cur_prep_lema"/>      <lit v=""/></let>
    <let><var n="cur_syn_tag"/>        <lit-tag v="@X"/></let>
    <let><var n="EOS"/>                <lit v="true"/></let>
  </def-macro>
  
  <def-macro n="set_curr_syntag" npar="1">
    <choose> <!-- Because chunks of the same type should have the same tags
                  to make sure the pointers point to the right places -->
      <when>
        <test>
            <equal caseless="yes">
              <clip pos="1" side="sl" part="syntax"/>
              <lit v=""/>
            </equal>
        </test>
        <let>
          <var n="cur_syn_tag"/>
          <lit-tag v="@X"/>
        </let>
      </when>
      <otherwise> 
        <let>
          <var n="cur_syn_tag"/>
          <clip pos="1" side="sl" part="syntax"/>
        </let>
      </otherwise> 
    </choose>

    <choose>
      <when>
        <test>
          <or>
            <equal>
              <clip pos="1" side="sl" part="syntax"/>
              <lit-tag v="@←SUBJ"/>
            </equal>
            <equal>
              <clip pos="1" side="sl" part="syntax"/>
              <lit-tag v="@SUBJ→"/>
            </equal>
          </or>
        </test>
        <let>
          <var n="cur_subj"/>
          <concat>
            <clip pos="1" side="sl" part="gen"/>
            <clip pos="1" side="sl" part="nbr"/>
          </concat>
        </let>
      </when>

    </choose>
  </def-macro>

  <def-macro n="firstWord" npar="1">
      <choose>
        <when>
          <test>
              <equal>
                <clip pos="1" side="sl" part="a_np_acr"/>
                <lit v=""/>
              </equal>
          </test>
          <choose>
            <when>
              <test>
                <equal> 
                  <var n="EOS"/>
                  <lit v="true"/>
                </equal> 
              </test>
              <modify-case>
                <clip pos="1" side="tl" part="lem"/>
                <lit v="aa"/>
              </modify-case>
              <let>
                <var n="caseFirstWord"/>
                <lit v="Aa"/>
              </let>
            </when>
            <otherwise> 
              <let>
                <var n="caseFirstWord"/>
                <lit v="aa"/>
              </let>
            </otherwise>
          </choose>
        </when>
        <otherwise>
          <let>
            <var n="caseFirstWord"/>
            <lit v="aa"/>
          </let>
        </otherwise>
      </choose>
      <let>
        <var n="EOS"/>
        <lit v="false"/>
      </let>
  </def-macro>

  <def-macro n="set_cur_adj_grau1" npar="1">
      <let>
        <var n="cur_adj_grau_preadv1"/>
        <lit v=""/>
      </let>
      <let>
         <var n="cur_adj_grau1"/>
         <clip pos="1" side="tl" part="grau"/>
      </let>
      <choose>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="sl" part="grau"/>
                <lit-tag v="comp"/>
              </equal>
            </and>
          </test>
          <append n="cur_adj_grau_preadv1">
            <lit v="more"/>
            <lit-tag v="preadv"/>
            <lit v="$ ^"/>
          </append>
          <let>
            <var n="cur_adj_grau1"/>
            <lit v=""/>
          </let>
        </when>
        <when>
          <test>
            <and>
              <not>
                <equal>
                  <clip pos="1" side="tl" part="a_adj"/>
                  <lit-tag v="adj.sint"/>
                </equal>
              </not>
              <equal>
                <clip pos="1" side="sl" part="grau"/>
                <lit-tag v="sup"/>
              </equal>
            </and>
          </test>
          <append n="cur_adj_grau_preadv1">
            <lit v="most"/>
            <lit-tag v="preadv"/>
            <lit v="$ ^"/>
          </append>
          <let>
            <var n="cur_adj_grau1"/>
            <lit v=""/>
          </let>
        </when>
        <otherwise>
          <append n="cur_adj_grau_preadv1">
            <lit v=""/>
          </append>
        </otherwise>
      </choose>
  </def-macro>

  <def-macro n="conv_vera_to_hafa1" npar="1">
     <choose>
      <when>
        <test>
          <equal caseless="yes">
            <clip pos="1" side="sl" part="lem"/>
            <lit v="vera"/> 
          </equal>
        </test>
        <let>
          <clip pos="1" side="sl" part="lem"/>
          <lit v="hafa"/> 
        </let>
        <let>
          <clip pos="1" side="tl" part="lem"/>
          <lit v="have"/> 
        </let>
        <let>
          <clip pos="1" side="sl" part="a_verb"/>
          <lit-tag v="vbhaver"/> 
        </let>
        <let>
          <clip pos="1" side="tl" part="a_verb"/>
          <lit-tag v="vbhaver"/> 
        </let>
      </when>
    </choose>
  </def-macro>

  <def-macro n="conv_ætla_to_be1" npar="1">
        <let>
          <clip pos="1" side="sl" part="lem"/>
          <lit v="vera"/> 
        </let>
        <let>
          <clip pos="1" side="tl" part="lem"/>
          <lit v="be"/> 
        </let>
        <let>
          <clip pos="1" side="sl" part="a_verb"/>
          <lit-tag v="vbser"/> 
        </let>
        <let>
          <clip pos="1" side="tl" part="a_verb"/>
          <lit-tag v="vbser"/> 
        </let>
