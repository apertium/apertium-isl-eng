<rule comment="REGLA: PrnPos"><!-- rule for personal pronoun -->
   <pattern>
     <pattern-item n="prnpers"/>
   </pattern>
   <action>
     <call-macro n="firstWord">
       <with-param pos="1"/>
     </call-macro>
     <call-macro n="set_curr_syntag">
       <with-param pos="1"/>
     </call-macro>
     <choose>
      <when>
        <test>
          <and>
            <or>
              <equal>
                <clip pos="1" side="sl" part="case"/>
                <lit-tag v="acc"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="case"/>
                <lit-tag v="dat"/>
              </equal>
              <equal>
                <clip pos="1" side="sl" part="case"/>
                <lit-tag v="gen"/> <!-- til þeirra -->
              </equal>
            </or>
            <not>
              <or>
                <equal>
                  <clip pos="1" side="sl" part="syntax"/>
                  <lit-tag v="@SUBJ→"/>
                </equal>
                <equal>
                  <clip pos="1" side="sl" part="syntax"/>
                  <lit-tag v="@←SUBJ"/>
                </equal>
              </or>
            </not>
          </and>
        </test>
        <let>
           <clip pos="1" side="tl" part="a_prn"/>
           <lit-tag v="prn.obj"/>
        </let>
      </when>
      <otherwise>
        <let>
           <clip pos="1" side="tl" part="a_prn"/>
           <lit-tag v="prn.subj"/>
        </let>
        <let>
           <var n="cur_subj_prn"/>
           <concat>
             <clip pos="1" side="sl" part="pers"/>
             <clip pos="1" side="sl" part="gen"/>
             <clip pos="1" side="sl" part="nbr"/>
           </concat>
        </let>
        <let>
          <var n="cur_subj_gen"/>
          <clip pos="1" side="sl" part="gen"/>
        </let>
      </otherwise>
     </choose>
     <choose>
      <when>
      <test>
          <and>
            <equal>
              <clip pos="1" side="sl" part="pers"/>
              <lit-tag v="p3"/>
            </equal>
            <equal>
              <clip pos="1" side="sl" part="nbr"/>
              <lit-tag v="pl"/>
            </equal>
          </and>
      </test>
        <let>
          <clip pos="1" side="tl" part="gen"/>
          <lit-tag v="mf"/>
        </let>
      </when>
     </choose>
     <choose>
      <when>
       <test>
        <equal>
            <clip pos="1" side="sl" part="pers"/>
          <lit-tag v="p2"/>
        </equal>
       </test>
       <let>
         <clip pos="1" side="tl" part="nbr"/>
         <lit-tag v="sp"/>
       </let>
      </when>
     </choose>
     <out>
       <chunk name="prn" case="caseFirstWord">
         <tags>
            <tag><lit-tag v="SN"/></tag>
            <tag><var n="cur_syn_tag"/></tag>
